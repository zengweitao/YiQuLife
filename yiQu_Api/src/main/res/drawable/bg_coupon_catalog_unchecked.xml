<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F2F2F2"></solid>
            <stroke
                android:width="1px"
                android:color="#CDCDCD"></stroke>
        </shape>
    </item>
    <item
        android:bottom="1px"
        android:left="1px"
        android:right="1px">
        <shape android:shape="rectangle">
            <solid android:color="#F2F2F2"></solid>
        </shape>
    </item>
</layer-list>